                                <div class="img">
                                    <a href="${item.purl}" target="_blank">
                                        <img alt="${item.pn}" src="${item.iurl}">
                                    </a>
                                </div>
                                <div class="link">
                                    <p class="link_name">
                                        <a target="_blank" href="${item.purl}">${item.pn}</a></p>
                                    <p class="price">
                        <span class="price_val">
                            #if(${item.price}&&${item.price}!="")
                            &#165;<strong>$number.format('.00',${item.price})</strong>
