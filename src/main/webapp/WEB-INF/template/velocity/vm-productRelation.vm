                    #if(!${skuId})
                        <h2><a href="//item.gome.com.cn/${title_key.goodsId}.html" target="_blank">${title_key.goodsName}</a></h2>
                    #else
                        <h2><a href="//item.gome.com.cn/${title_key.goodsId}-${skuId}.html" target="_blank">${title_key.goodsName}</a></h2>
                    #end
