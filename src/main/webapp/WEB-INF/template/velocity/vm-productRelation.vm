                    #set($domainsite = '//item.gome.com.cn/')
                    <script type="text/javascript" src="//js.gomein.net.cn/eccms/js/topics_js/14/2360346386.min.js"></script>
                    <script type="text/javascript" style="">
                        var skuIdAttr=[];
                    </script>
                    #if($productRelation && $productRelation.size() > 0)
                        #foreach($item in $productRelation)
                            #if($velocityCount == 1)
                            <div class="item first">
                            #else
                                    <div class="item">
                            #end
                                <div class="inf">
                                <a class="img" href="${domainsite}${item.id}-${item.skuId}.html" target="_blank"><img src="${item.imgUrl}"></a>
